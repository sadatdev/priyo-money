{
    "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "react-hooks/rules-of-hooks": "error",
        "react-hooks/exhaustive-deps": "warn",
        "prettier/prettier": [
